  <style>
    @font-face {
      font-family: 'Hind';
      font-display: fallback;
      font-style: normal;
      font-weight: 400;
      src: local('Hind Regular'), local('Hind-Regular'),
          url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/hind.woff2') format('woff2'),
          url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/hind.woff') format('woff');
    }

    @font-face {
      font-family: 'Lora';
      font-display: fallback;
      font-style: normal;
      font-weight: 400;
      src: local('Lora Regular'), local('Lora-Regular'),
          url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/lora.woff2') format('woff2'),
          url('<%= htmlWebpackPlugin.options.pathname %>/assets/fonts/lora.woff') format('woff');
    }

    @font-face {
      font-family: 'Kotori Rose';
      font-display: fallback;
      font-style: normal;
